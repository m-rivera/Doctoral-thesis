@article{A.D.Becke1988,
abstract = {Current gradient-corrected density-functional approximations for the exchange energies of atom- ic and molecular systems fail to reproduce the correct 1/r asymptotic behavior of the exchange- energy density. Here we report a gradient-corrected exchange-energy functional with the proper asymptotic limit. Our functional, containing only one parameter, fits the exact Hartree-Fock ex- change energies of a wide variety of atomic systems with remarkable accuracy, surpassing the per- formance of previous functionals containing two parameters or more.},
annote = {B88},
author = {Becke, A. D.},
