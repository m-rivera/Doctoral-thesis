doi = {10.1021/acs.jpcc.7b08060},
issn = {19327455},
journal = {J. Phys. Chem. C},
number = {41},
pages = {23166--23183},
title = {{Solid State Luminescence Enhancement in $\pi$-Conjugated Materials: Unraveling the Mechanism beyond the Framework of AIE/AIEE}},
volume = {121},
year = {2017}
}

@article{Plasser2012,
abstract = {A procedure for a detailed analysis of excited states in systems of interacting chromophores is proposed. By considering the one-electron transition density matrix, a wealth of information is recovered that may be missed by manually analyzing the wave function. Not only are the position and spatial extent given, but insight into the intrinsic structure of the exciton is readily obtained as well. For example, the method can differentiate between excitonic and charge resonance interactions even in completely symmetric systems. Four examples are considered to highlight the utility of the approach: interactions between the n$\pi$* states in a formaldehyde dimer, excimer formation in the naphthalene dimer, stacking interaction in an adenine dimer, and the excitonic band structure in a conjugated phenylenevinylene oligomer.},
author = {Plasser, Felix and Lischka, Hans},
doi = {10.1021/ct300307c},
issn = {15499626},
journal = {J. Chem. Theory Comput.},
number = {8},
pages = {2777--2789},
title = {{Analysis of excitonic and charge transfer interactions from quantum chemical calculations}},
volume = {8},
year = {2012}
}




