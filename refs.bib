month = {jan},
number = {50},
pages = {19357--19363},
title = {{ONIOM: A Multilayered Integrated MO + MM Method for Geometry Optimizations and Single Point Energy Predictions. A Test for Diels−Alder Reactions and Pt(P( t -Bu) 3 ) 2 + H 2 Oxidative Addition}},
url = {https://pubs.acs.org/doi/10.1021/jp962071j},
